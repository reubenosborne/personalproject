{
  "manifest_version": 2,
  "name": "Personal Project",
  "description": "A chrome extension to practice API's for EDA Personal Project",
  "version": "1.0.0.0",
  "author": "Reuben Osborne",
  "browser_action": {
    "default_icon": "tab-icon.svg",
    "default_title": "A+",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["activeTab", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}